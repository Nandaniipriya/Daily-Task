    "def star(n):\n",
    "    j=[]\n",
    "    for i in range(n):\n",
    "        j.append(' '*(n-i-1)+'*'*(2*i+1)+' '*(n-i-1))\n",
    "    for i in range(n-1):\n",
    "        j.append(' '*(i+1) + '*'*(2*(n-1-i)-1) + ' '*(i+1))\n",
    "    return j\n",
    "    \n",
    "print(star(3))"
   ]
  }
