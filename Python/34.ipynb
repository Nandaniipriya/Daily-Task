    "def merge(dict1, dict2, dict3):\n",
    "    for i,j in dict1.items():\n",
    "        dict2[i]=j\n",
    "    for i,j in dict3.items():\n",
    "        dict2[i]=j\n",
    "    return dict2\n",
    "    \n",
    "dict1={'a': 1, 'b': 2} \n",
    "dict2={'c': 3, 'd': 4}\n",
    "dict3={'e': 5, 'f': 6}\n",
    "print(merge(dict1,dict2,dict3))"
   ]
