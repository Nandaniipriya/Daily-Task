   "source": [
    "def star(lst):\n",
    "    even=0\n",
    "    odd=0\n",
    "    for i in lst:\n",
    "        if i % 2 == 0:\n",
    "           even=even+1\n",
    "        else:\n",
    "           odd=odd+1\n",
    "    return even,odd\n",
    "\n",
    "print(star([2, 5, 6]))"
   ]
  }
