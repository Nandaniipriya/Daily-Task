    "def star(n):\n",
    "    j=[]\n",
    "    j.append('*'*n)\n",
    "    for i in range(n-3):\n",
    "        j.append('*'+' '*(n-i-3)+'*')\n",
    "    j.append(('**'))\n",
    "    j.append('*')\n",
    "    return j\n",
    "    \n",
    "print(star(5))\n",
    "    "
   ]
