    "def star(n):\n",
    "    jn=[]\n",
    "    for i in range (n):\n",
    "        spaces=' '*(n-i-1)\n",
    "        j=[]\n",
    "        for k in range (i+1):\n",
    "            j.append(str(k+1))\n",
    "        numbers=' '.join(j)\n",
    "        jn.append(spaces+str(numbers)+spaces)\n",
    "     \n",
    "    return jn\n",
    "print(star(3))"
   ]
  }
